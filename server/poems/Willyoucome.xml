      <div xml:id="Will you come?"> 
              <interpGrp type="poembox">
                <interp ana="#form" type="sonnet"></interp>
                <interp ana="#type" type="sad"></interp>
                <interp ana="#topic" type="love"></interp>
              </interpGrp>
             <lg> 
              <l>Will you come?,</l>
          <l>Will you come?,</l> 
          <l>Will you ride</l>
          <l>so late,</l>
          <l>at my side?</l>
          <l>Will you come?</l>
          <l>Will you come? </l>
          
            <l>Will you come?</l>
          <l>if the night</l>
          <l>has a moon </l>
          <l>Full and bright?</l>
          <l>Will you come? </l>
          
                 
               <!-- You can put comments or notes to yourself in like this -->
             </lg>
