  <text>
      <body>
        
     
      <div xml:id="Dug"> 
              <interpGrp type="poembox">
                <interp ana="#form" type="free"></interp>
                <interp ana="#tone" type="sad"></interp>
                <interp ana="#topic" type="society"></interp>
              </interpGrp>
             <lg> 
              <l>i have dug my way</l>
          <l>out the ground</l> 
          <l>with palm and fist many times</l>
          <l>my whole life has been</l>
          <l>one burial after another</l>
          <l>i will find my way</l>
          <l>out of you just fine</l>
    
                <!-- You can put comments or notes to yourself in like this -->
             </lg>
