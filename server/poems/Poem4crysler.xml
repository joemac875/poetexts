      <titleStmt>
        <title>Do Not Stand by My Grave and Weep</title>
        <author>
          <persName>Mary Elizabeth Frye</persName>
        </author>
      </titleStmt>
      <publicationStmt>
          <ab></ab>
      </publicationStmt>
