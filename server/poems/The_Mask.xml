    <text>
        <body>
            
            
            <div xml:id="Poem4"> 
                <interpGrp type="poembox">
                    <interp ana="#form" type="free"></interp>
                    <interp ana="#tone" type="sad"></interp>
                    <interp ana="#topic" type="love"></interp>
                </interpGrp>
                <lg> 
                    <l>She smiles, I cry</l>
                    <l>She is outgoing, I am shy.</l>
                    <l>She loves, I am alone.</l>
                    <l>She is amazing, I am unkown.</l>
                    <l>She is beautiful, I am a mess.</l>
                    <l>She is happy, I am depressed.</l>
                    <l>My mask is perfect.</l>
                    <l>She hides me.</l>
                    
                    
                    
                    <!-- You can put comments or notes to yourself in like this -->
                </lg>
            </div>
            
        </body>
