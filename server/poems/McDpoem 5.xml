  <text>
      <body>
        
     
      <div xml:id="Sonnet41"> 
              <interpGrp type="poembox">
                <interp ana="#form" type="haiku"></interp>
                <interp ana="#tone" type="sad"></interp>
                <interp ana="#topic" type="love"></interp>
              </interpGrp>
             <lg> 
              <l>what i want</l>
          <l>from you can</l> 
          <l>you give? what</l>
          <l>i give to </l>
          <l>you do you</l>
          <l>want? hey? hey?</l>
                 
               <!-- You can put comments or notes to yourself in like this -->
             </lg>
